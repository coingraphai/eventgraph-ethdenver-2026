# Backend Environment Variables - Example Configuration
# Copy this to .env and fill in your actual values

# APP SETTINGS
ENVIRONMENT=development
APP_NAME=Event Graph API

# DATABASE (PostgreSQL)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=eventgraph
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_password_here

# API KEYS
ANTHROPIC_API_KEY=sk-ant-your-key-here
DOME_API_KEY=your_dome_key_here

# MARKET DATA SETTINGS (HACKATHON)
ARBITRAGE_TOP_N_MARKETS=500
ARBITRAGE_MIN_VOLUME_USD=10000
DEMO_MODE_TOP_N=100
